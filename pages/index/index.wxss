page {
  background: url(https://php-yangyi-images.oss-cn-shenzhen.aliyuncs.com/qinqi/assets/images/img_home_bg.png) center center no-repeat;
  background-size: cover;
}

.index .topBgGradient .gpsTip {
  text-align: right;
  font-size: 28rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  color: #17233e;
  line-height: 20rpx;
  padding: 40rpx;
  position: relative;
}
.index .topBgGradient .gpsTip view {
  display: inline-block;
}
.index .topBgGradient .gpsTip .icon {
  width: 8rpx;
  height: 16rpx;
  opacity: 1;
  background: #1dc779;
  border-radius: 1rpx;
  display: inline-block;
  margin-left: 5rpx;
  vertical-align: middle;
}
.index .topBgGradient .kilometre {
  display: block;
  padding-top: 65rpx;
}
.index .topBgGradient .kilometre text {
  display: block;
  text-align: center;
}
.index .topBgGradient .kilometre .text {
  font-size: 28rpx;
  line-height: 40rpx;
  padding: 30rpx;
  color: #7a7d9e;
}
.index .topBgGradient .kilometre .num {
  font-size: 160rpx;
  line-height: 150rpx;
  font-family: DINCond;
  font-weight: 700;
  color: #181a39;
}
.index .botBgGradient {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
}
.index .botBgGradient view {
  display: block;
}
.index .botBgGradient .bottomBox {
  width: 100%;
  margin-top: 100rpx;
  margin-bottom: 100rpx;
  text-align: center;
  font-size: 28rpx;
}
.index .botBgGradient .bottomBox .myBox {
  display: inline-block;
  vertical-align: middle;
}
.index .botBgGradient .bottomBox .myBox icon {
  background: #ffffff url(data:image/png;base64,) center center no-repeat;
  background-size: 60rpx 60rpx;
  width: 100rpx;
  height: 100rpx;
  display: block;
  margin-bottom: 12rpx;
  border-radius: 50%;
  box-shadow: 0 1rpx 6rpx 1rpx rgba(0, 0, 0, 0.09);
  font-size: 0;
}
.index .botBgGradient .goExercise {
  width: 300rpx;
  height: 300rpx;
  margin: 0 30rpx;
  vertical-align: middle;
  display: inline-block;
}
.index .botBgGradient .goExercise .startIcon {
  background: url(data:image/png;base64,) no-repeat;
  background-size: contain;
}
.index .botBgGradient .goExercise icon {
  width: 300rpx;
  height: 300rpx;
  -webkit-box-shadow: none;
  box-shadow: none;
  display: inline-block;
  font-size: 0;
}
.index .botBgGradient .pathBox {
  display: inline-block;
  vertical-align: middle;
}
.index .botBgGradient .pathBox icon {
  background: #ffffff url(data:image/png;base64,) center center no-repeat;
  background-size: 60rpx 60rpx;
  width: 100rpx;
  height: 100rpx;
  display: block;
  margin-bottom: 12rpx;
  border-radius: 50%;
  box-shadow: 0 1rpx 6rpx 1rpx rgba(0, 0, 0, 0.09);
  font-size: 0;
}

